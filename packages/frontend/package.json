{
  "name": "@scrabble-solver/frontend",
  "version": "1.4.0",
  "description": "Scrabble Solver - Frontend",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "src/:src/modules/"
      }
    },
    "build:prod": {
      "command": "react-scripts build",
      "env": {
        "API_HOST": "http://kamilmielnik.com",
        "API_PORT": "5000",
        "NODE_ENV": "production",
        "NODE_PATH": "src/:src/modules/"
      }
    },
    "start": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "src/:src/modules/"
      }
    },
    "start:prod": {
      "command": "http-server dist",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/scrabble-solver.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/scrabble-solver/issues"
  },
  "homepage": "https://github.com/kamilmielnik/scrabble-solver#readme",
  "dependencies": {
    "@scrabble-solver/configs": "1.4.0",
    "@scrabble-solver/constants": "1.4.0",
    "@scrabble-solver/models": "1.4.0",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "re-reselect": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-joyride": "^1.11.4",
    "react-redux": "^7.1.0",
    "react-virtualized": "^9.21.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.0.4",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "better-npm-run": "0.1.1",
    "http-server": "^0.11.1",
    "node-sass": "^4.12.0",
    "react-scripts": "^3.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
