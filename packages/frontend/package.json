{
  "name": "@scrabble-solver/frontend",
  "version": "2.0.0-alpha.6",
  "description": "Scrabble Solver - Frontend",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production",
        "REACT_APP_API_URL": "http://localhost:5000/api"
      }
    },
    "build:prod": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production",
        "REACT_APP_API_URL": "http://kamilmielnik.com:5000/api"
      }
    },
    "start": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "development",
        "REACT_APP_API_URL": "http://localhost:5000/api"
      }
    },
    "start:prod": {
      "command": "http-server build",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/scrabble-solver.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/scrabble-solver/issues"
  },
  "homepage": "https://github.com/kamilmielnik/scrabble-solver#readme",
  "dependencies": {
    "@scrabble-solver/configs": "2.0.0-alpha.6",
    "@scrabble-solver/constants": "2.0.0-alpha.6",
    "@scrabble-solver/models": "2.0.0-alpha.6",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-use": "^15.3.4",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "store2": "^2.12.0"
  },
  "devDependencies": {
    "better-npm-run": "0.1.1",
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0",
    "react-scripts": "^4.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
